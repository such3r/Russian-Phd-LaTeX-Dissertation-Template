journal = {Journal of the American Chemical Society},
keywords = {Hydrophobic and Hydrophilic Interactions,Models, Molecular,Principal Component Analysis,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Thermodynamics,Water,Water: chemistry},
