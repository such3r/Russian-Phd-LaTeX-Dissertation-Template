doi = {10.1002/humu.20950},
file = {:home/nikolay/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Corrado et al. - 2009 - High frequency of TARDBP gene mutations in italian patients with amyotrophic lateral sclerosis.pdf:pdf},
