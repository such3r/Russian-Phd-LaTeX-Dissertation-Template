issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {ALS, amyloid,ALS, amyotrophic lateral sclerosis,Cu, Zn superoxide dismutase,FALS, familial ALS,SALS, sporadic ALS,SOD1,SOD1, human cytosolic Cu,Zn superoxide dismutase,familial amyotrophic lateral sclerosis,wtSOD1, wild-type SOD1},
