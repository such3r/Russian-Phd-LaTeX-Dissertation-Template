issn = {1520-5126},
journal = {Journal of the American Chemical Society},
keywords = {Molecular Dynamics Simulation,Protein Folding,Protein Structure, Secondary,Proteins,Proteins: chemistry,Solvents,Solvents: chemistry,implicit water},
language = {english},
